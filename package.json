{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "bootstrap:dev": "cross-env UMI_ENV=dev node scripts/init.ts ",
    "bootstrap:prod": "cross-env UMI_ENV=prod node scripts/init.ts ",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "antd": "^4.17.4",
    "img-compress": "^1.0.0",
    "js-moment": "^2.2.2",
    "qs": "^6.10.1",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.5.20"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
